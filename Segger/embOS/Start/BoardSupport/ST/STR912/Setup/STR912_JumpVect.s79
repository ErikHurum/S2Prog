        NAME    vectjump
        RSEG    INTVEC:CODE
        CODE32


ResetLoop:
        B       ResetLoop;              ; Ensure CPU does not run into uninitialized RAM after reset
        LDR     PC, =0x4000004          ; Jump into SRAM, D-TCM
        LDR     PC, =0x4000008
        LDR     PC, =0x400000C
        LDR     PC, =0x4000010
        NOP                             ; Reserved vector
        LDR     PC, =0x4000018
        LDR     PC, =0x400001C

        LTORG
        END
