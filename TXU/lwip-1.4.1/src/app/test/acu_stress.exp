#!/usr/bin/expect

proc do_acu {} {
   spawn telnet 192.168.1.3 4002
   expect "Username : "

   send "masteruser\r"
   expect "masteruser\r"

   expect "Password : "

   send "masteruser\r"
   expect "**********\r"

   interact timeout 1800 return

   send "q\r"
   expect eof
}

set timeout -1

while { 1 } {
   do_acu

   sleep 3
}
