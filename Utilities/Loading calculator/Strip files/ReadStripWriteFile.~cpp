//---------------------------------------------------------------------------

#include <vcl.h>
#pragma hdrstop
USERES("ReadStripWriteFile.res");
USEFORM("ReadFileAndStrip.cpp", ReadFileFormat);
USEUNIT("\\TSN_SERVER\S2K\Utilities Loading calculator\Strip files\Utilities.CPP");
//---------------------------------------------------------------------------
WINAPI WinMain(HINSTANCE, HINSTANCE, LPSTR, int)
{
  try
  {
     Application->Initialize();
     Application->CreateForm(__classid(TReadFileFormat), &ReadFileFormat);
     Application->Run();
  }
  catch (Exception &exception)
  {
     Application->ShowException(&exception);
  }
  return 0;
}
