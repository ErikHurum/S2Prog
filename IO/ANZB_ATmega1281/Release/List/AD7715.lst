     40          static inline signed char
     41          fcompare(float a, float b) {
     42              //
     43              // returns -1 when a < b
     44              //          1 when a > b
     45              //          0 when equal
     46              //
     47          #define FLT_EPSILON 0.000001f
     48          
     49              float d = a - b;
     50          
     51              if (d >= -FLT_EPSILON && d <= FLT_EPSILON) {
     52                  return 0;
     53              }
     54          
     55              if (d < -FLT_EPSILON) {
     56                  // a is less than b
     57                  return -1;
     58              }
     59          
     60              // a is bigger than b
     61              return 1;
     62          }
