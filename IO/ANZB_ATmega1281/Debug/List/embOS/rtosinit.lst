    332          *       OS_ISR_rx_Handler()
    333          *       embOS UART rx interrupt handler
    334          */
    335          void OS_ISR_rx_Handler(void);
    336          void OS_ISR_rx_Handler(void) {
    337              UCSR0B &=  ~(1 << 7);     // Avoid nesting itself !!!
    338              OS_OnRx(UDR0);
    339              OS_DI();             // Disable before allowing Rx interrupt
    340              UCSR0B |=  (1 << 7);
