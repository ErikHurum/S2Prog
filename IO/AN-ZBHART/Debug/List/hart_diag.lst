    205          static uint8_t ad7715_wait_for_drdy(void) {
    206              char Status = OS_WaitSingleEventTimed(HART_DIAG_CONVERSION_COMPLETE, 1000);
    207              if (!Status) {
    208                  Last_ADC_FuckUpTime = OS_Time;
    209                  _num_adc_fuckups++;
    210              } else {
    211                  Last_ADC_OK_Time = OS_Time;
    212              }
    213              return Status;
    214          }
